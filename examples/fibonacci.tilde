function fibonacci ~n (
    if ~n <= 1 (
        give ~n
    )
    ~a is 0
    ~b is 1
    ~i is 2
    loop (
        if ~i > ~n break-loop
        ~temp is ~b
        ~b is ~a + ~b
        ~a is ~temp
        ~i is ~i + 1
    )
    give ~b
)

say "Fibonacci sequence:"
~i is 0
loop (
    if ~i > 10 break-loop
    ~fib is *fibonacci ~i
    say "F(" ~i ") = " ~fib
    ~i is ~i + 1
)
